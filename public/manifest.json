{
  "manifest_version": 3,
  "name": "Clio",
  "version": "1.0",
  "description": "An easy to use bookmark manager.",
  "permissions": ["bookmarks"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
